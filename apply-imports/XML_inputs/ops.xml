<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="ops.xsl"?>
<ops>
	<desc>Some binary operations</desc>
	<op name="add" symbol="+">
		<operand>1</operand>
		<operand>2</operand>
	</op>
	<op name="sub" symbol="-">
		<operand>1</operand>
		<operand>2</operand>
	</op>
	<op name="mul" symbol="*">
		<operand>1</operand>
		<operand>2</operand>
	</op>
</ops>